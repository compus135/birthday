/* pages/game/index.wxss */
.game-wrapper{
  width:100vw;
  height:100vh;
  color:#fff;
  position: relative;
  overflow: hidden;
  /* background:linear-gradient(to right,#ac00ff,#ff00d5,#ff0072,#ff0072); */
  background: #f5f5f5;
  box-sizing: border-box;
}
.game-scoreborad{
  height:100vh;
  width:100vw;
  position: relative;
  z-index: 23;background:linear-gradient(to right,#ac00ff,#ff00d5,#ff0072,#ff0072);
}

.game-scores-main{
  background: rgba(0,0,0,0.3);
  height:70vh;
  width:80vw;
  display: block;
  /* display: flex;
  align-items: flex-start;
  justify-content: center;
  flex-direction: column; */
  position: absolute;
  top:15vh;
  left:10vw;
  border-radius: 10px;
  overflow-y: scroll;
    box-sizing: border-box;
    padding:0 5px;
}
.game-scoreborad-title{
  position: absolute;
  left:calc(50vw - 100px);
  top:calc(15vh - 36px);
  width:200px;
  height:30px;
  background:rgba(0,0,0,0.3);
  color:#fff;
  border-radius: 90px;
  text-align: center;
  line-height: 30px;
  font-size: 16px;
  z-index: 2;
}
.game-scoreborad-close{
  position: absolute;
  left:calc(50vw - 25px);
  bottom:calc(15vh - 56px);
  width:50px;
  height:50px;
  background:rgba(0,0,0,0.7);
  color:#fff;
  border-radius: 90px;
  text-align: center;
  line-height: 50px;
  font-size: 30px;
  z-index: 2;
}

.game-score-item{
  display: flex;
  align-items: center;
  /* background: red; */
    width: 100%;
    padding:8px 12px;
    box-sizing: border-box;
    height:66px;
    border-bottom: 1px solid #eee;
}
.game-score-order{width: 30px}
.game-score-avatar{
  width: 60px;
  display: flex;
  justify-content: center;
  overflow: hidden;
}
.game-score-avatar image{
  width:50px;
  height:50px;
  border-radius: 50%;
}
.game-score-name{flex:1}
.game-score-score{width:30px}
.game-home{
  background:linear-gradient(to bottom,#ac00ff,#ff00d5,#ff0072,#ff0072);
  height:100vh;
  width:100vw;
  position: relative;
  z-index: 3;

}
.game-home-main{
  background: rgba(0,0,0,0.3);
  height:70vh;
  width:80vw;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  position: absolute;
  top:15vh;
  left:10vw;
  border-radius: 10px;
}
.game-home-main>div{
    margin-top:30px;
}
.game-home-tip{
  color:#eee;
  font-size:12px;
}
.game-home-title{
  font-size: 14px;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.game-home-title>div{
  display: inline-block;
  margin:8px 0;
  text-align: center;
}
.game-scores-switch{
  margin-top:20px!important;
  color:#fff;
  font-size:14px;
  border-radius: 20px;
}

.game-play{
  background:linear-gradient(to right,#ff0072,#ac00ff);
  color:#fff;
  border-radius: 100px;
  padding:15px 50px!important;
  box-shadow: 0 0 15px #fff;
}

.game-playagain{
    background:linear-gradient(to right,#ff0072,#ac00ff);
    color:#fff;
    border-radius: 100px;
    padding:15px 50px!important;
    box-shadow: 0 0 15px #fff;
}

.game-over{
  height:100vh;
  width:100vw;
  position: relative;

}
.game-over-main{
  background: rgba(0,0,0,0.3);
  height:70vh;
  width:80vw;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  position: absolute;
  top:15vh;
  left:10vw;
  border-radius: 10px;
}

.game-over-main>div{
    margin-top:30px;
}

.game-score{
  background:rgba(0,0,0,0.3);
  color:#fff;
  position: absolute;
  top:35px;
  left:calc(50% - 30px);
  width:60px;
  border-radius: 100px;
  text-align: center;
  z-index: 2;
  padding:5px 10px;
}
.game-replay{
    color:#000;
    position: absolute;
    top:35px;
    left:10px;
    width:70px;
    border-radius: 100px;
    text-align: center;
    z-index: 2;
    padding:5px 0;
}

.game-canvas{
  position: absolute;
  bottom:100%;
  left:5vw;
  width:90vw;
  display: flex;
  flex-wrap: wrap;
  align-content: flex-start;
}
.game-canvas-play{
  bottom:calc(100vh - 15000px);
  transition: all 10s linear;
}
.game-item{
  display: flex;
  align-items: center;
  justify-content: center;
  width:25%;
  height:200px;
  background-position: center center!important;
  /* background:blue; */
  /* box-shadow: 0 0 3px #fff; */
}
